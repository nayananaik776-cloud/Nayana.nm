#include <stdio.h>
#include <string.h>
int main()
{
    char str1[30] = "Hello";
    char str2[30] = "World";
    printf("length of str1 : %d\n", strlen(str1));
    strcat(str1, str2);
    printf("After concatenation : %s\n", str1);
    strcpy(str2, str1);
    printf("Copied string : %s\n", str2);
    printf("Comparison result : %d\n", strcmp(str1, str2));
   return 0;
}
